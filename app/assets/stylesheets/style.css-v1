		filter: dropshadow(color=#b7e27b, offx=0, offy=0);
	}
	
	
	#specialities strong{
		font-size:20px;
		text-shadow: 1px 1px 1px #b7e27b;
		filter: dropshadow(color=#b7e27b, offx=0, offy=0);
	}
	
	#specialities .title{
		margin:60px 0 20px 0;
	}
	
	.margin-bottom{
		margin-bottom:90px;
	}
	
	/* Circles */

	#specialities .three{
		background-color: rgb(217,242,179); opacity: 0.9;
		width: 12em; 
		height: 12em; 
		-webkit-border-radius: 7em; 
		-moz-border-radius: 7em;
		margin-left:7%;
		border:4px solid #68ac25;
		background:url(../images/icon-tooltip-off.png) no-repeat center 10px #f0fcdd;
		-webkit-box-shadow:none;
		-moz-box-shadow:none;
		box-shadow:none;
	}
	

	#specialities .three:hover{		
		-webkit-box-shadow: 0px 0px 10px 0px #888;
		-moz-box-shadow: 0px 0px 10px 0px #888;
		box-shadow: 0px 0px 10px 0px #888;
		background:url(../images/icon-tooltip.png) #f0fcdd no-repeat center 10px;
	}
	
	#specialities .three h3{
		text-transform:uppercase;
		text-shadow: 1px 1px 1px #f6f6f6;
		filter: dropshadow(color=#f6f6f6, offx=0, offy=0);
		font-size:15px;
		font-weight:bold;
		text-align:center;
		padding-top:70px;
	}
	
	#specialities .twolines h3{
		padding-top:60px;
	}

	
	.tooltip{
	  position: relative;
	  background: #eaeaea;
	  cursor: help;
	  display: inline-block;
	  text-decoration: none;
	  color: #222;
	  outline: none;
	}
	
	.tooltip span{
	  visibility: hidden;
	  position: absolute;
	  top: -200px;
	  left: 50%;
	  z-index: 999;
	  width: 230px;
	  margin-left: -127px;
	  padding: 10px;
	  border: 2px solid #ccc;
	  opacity: .9;
	  background-color: #ddd;
	  background-image: -webkit-linear-gradient(rgba(255,255,255,.5), rgba(255,255,255,0));
	  background-image: -moz-linear-gradient(rgba(255,255,255,.5), rgba(255,255,255,0));
	  background-image: -ms-linear-gradient(rgba(255,255,255,.5), rgba(255,255,255,0));
	  background-image: -o-linear-gradient(rgba(255,255,255,.5), rgba(255,255,255,0));
	  background-image: linear-gradient(rgba(255,255,255,.5), rgba(255,255,255,0));
	  -moz-border-radius: 4px;
	  border-radius: 4px;
	  -moz-box-shadow: 0 1px 2px rgba(0,0,0,.4), 0 1px 0 rgba(255,255,255,.5) inset;
	  -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.4), 0 1px 0 rgba(255,255,255,.5) inset;
	  box-shadow: 0 1px 2px rgba(0,0,0,.4), 0 1px 0 rgba(255,255,255,.5) inset;
	  text-shadow: 0 1px 0 rgba(255,255,255,.4);
	}
	
	.tooltip:hover{
	  border: 0; /* IE6 fix */
	}
	
	.tooltip:hover span{
	  visibility: visible;
	}
	
	.tooltip span:before,
	.tooltip span:after{
	  content: "";
	  position: absolute;
	  z-index: 1000;
	  bottom: -7px;
	  left: 50%;
	  margin-left: -8px;
	  border-top: 8px solid #ddd;
	  border-left: 8px solid transparent;
	  border-right: 8px solid transparent;
	  border-bottom: 0;
	}
	
	.tooltip span:before{
	  border-top-color: #ccc;
	  bottom: -8px;
	}
	
	#specialities .tooltip img{
		margin:auto;
	}		
	
	

}	/* end desktop only styles */

@media only screen and (max-width: 767px) { /* Mobile */	
	
	#specialities h3{
